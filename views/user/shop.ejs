                                <div class="shop__product__option__left">
                                    <p>Showing 1–<%=items_per_page  %>  of <%= totalproducts  %>  results</p>
                                </div>
                            </div>
                            <div class="col-lg-6 col-md-6 col-sm-6">
                                <div class="shop__product__option__right">
                                    <p>Sort by Price:</p>
                                    <select>
                                        <option value="">Low To High</option>
                                        <option value="">$0 - $55</option>
                                        <option value="">$55 - $100</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <% product.forEach(function(x) { %> 
                            <% if(x.status == "listed") { %>
                        <div class="col-lg-4 col-md-6 col-sm-6">
                            <div class="product__item">
                                <div class="product__item__pic set-bg" data-setbg="/<%= x.image  %> ">
                                    <ul class="product__hover">
                                        <li><a onclick="itemAdded('<%= x._id %>')" ><img src="/public/stylesheet/css/shopPage/icon/heart.png" alt=""></a></li>
                                        
                                        <!-- <li><a href="#"><img src="/public/stylesheet/css/shopPage/icon/search.png" alt=""></a></li> -->
                                    </ul>
                                </div>
                                <div class="product__item__text">
                                    <h6><%= x.name  %> </h6>
                                    <a href="/moveToCart/<%= x._id %>" class="add-cart">+ Add To Cart</a>
                                   
                                    <h5><%= x.price %> </h5>
                                    
                                </div>
                            </div>
                        </div>
                        <% } %> 
                        <% }) %> 
                        
                        
