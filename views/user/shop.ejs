    <script>
        function itemAdded(id){

            $.ajax({
                url: "/addToWishlist",
                data: {
                id
            },
            method: "post",
            success: (response) => {
            if (response.success) {
  
                Swal.fire({
                icon: "success",
                title: "Success",
                text: "item added to wishlist",
            });
  
            } else {
                Swal.fire({
                icon: "error",
                title: "error",
                text: "item already exist ",
            });
  
        }
    }})
    }
